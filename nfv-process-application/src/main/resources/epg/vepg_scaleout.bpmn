      <bpmn2:incoming>SequenceFlow_20</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_11</bpmn2:outgoing>
      <bpmn2:script><![CDATA[ 
			def respSTR = execution.getVariable('getVDCSResp')
			def slurper = new groovy.json.JsonSlurper()
			def respJson = slurper.parseText(respSTR)			 
			def map = execution.getVariable('ECM_NAMEID_MAP')	
			if(map == null){
				map = new HashMap<String,String>();
			}		  			 
			 respJson.vdc.vns.each {
			 item ->
			 	 map.put(item.name.replaceAll('-',''),item.id)
			}
			respJson.vdc.vms.each {
			 item ->
			 	 map.put(item.name,item.id)
			}
			map.put('tenantName',execution.getVariable('appConfigProps')['ecm.props.map.tenantName'])
			map.put('vdcId',	execution.getVariable('vdcID'))		
			 execution.setVariable('templateMap',map);
			]]></bpmn2:script>
